{
  "name": "wodby/wordpress-composer",
  "description": "Project template for WordPress projects with composer",
  "type": "project",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "",
      "role": ""
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal-composer/preserve-paths": true,
      "johnpbloch/wordpress-core-installer": true
    }
  },
  "require": {
    "composer/installers": "^1.2",
    "drupal-composer/preserve-paths": "^0.1",
    "johnpbloch/wordpress-core": "~6.0",
    "johnpbloch/wordpress-core-installer": "^2.0",
    "wpackagist-plugin/acf-options-for-polylang": "^1.1",
    "wpackagist-plugin/complianz-gdpr": "^6.5",
    "wpackagist-plugin/contact-form-7": "^5.8",
    "wpackagist-plugin/contact-form-7-skins": "^2.6",
    "wpackagist-plugin/drag-and-drop-multiple-file-upload-contact-form-7": "1.3.7.5",
    "wpackagist-plugin/duplicate-post": "^4.5",
    "wpackagist-plugin/multilingual-contact-form-7-with-polylang": "^1.0",
    "wpackagist-plugin/ninja-forms": "^3.7",
    "wpackagist-plugin/polylang": "^3.5",
    "wpackagist-plugin/show-current-template": "^0.5.2",
    "wpackagist-plugin/ultimate-social-media-icons": "^2.8",
    "wpackagist-plugin/webp-express": "^0.25.8",
    "wpackagist-plugin/woocommerce": "^8.5",
    "wpackagist-plugin/woocommerce-products-filter": "1.3.5.1",
    "wpackagist-plugin/wordpress-seo": "^21.7",
    "wpackagist-plugin/wpspeed": "^2.5",
    "wpackagist-theme/oceanwp": "^3.5",
    "wpengine/advanced-custom-fields-pro": "^6.2"
  },
  "extra": {
    "installer-paths": {
      "web/wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/wp-content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "preserve-paths": [
      "web/wp-config.php",
      "web/wp-content/uploads",
      "web/wp-content/plugins/revslider",
      "web/wp-content/themes/beautycorner"
    ],
    "wordpress-install-dir": "web"
  }
}
